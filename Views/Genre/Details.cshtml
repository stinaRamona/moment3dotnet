@model moment3dotnet.Models.GenreModel

@{
    ViewData["Title"] = "Details"; 
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
<h1>Om genren</h1>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Namn på genre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GenreName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="link-success">Ändra</a> |
    <a asp-action="Index" class="link-success">Tillbaka till genrelistan</a>
</div>
</body>
</html>
